# InfraRed configuration file
# ===========================

[defaults]
settings  = settings
modules   = library
roles     = roles
playbooks = playbooks

# list all the specs
[provisioner]
main_playbook = provision.yml
cleanup_playbook = cleanup.yml

[installer]
main_playbook = install.yml
cleanup_playbook = cleanup.yml

[tester]
main_playbook = test.yml
cleanup_playbook = cleanup.yml

[cleanup]
main_playbook = cleanup.yml

[tempest]
tests = identity
