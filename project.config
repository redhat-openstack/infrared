[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/rhosqeauto
	owner = group user/yfried-redhat
	read = group Anonymous Users
	read = group rhosqeauto-core
	push = deny group Registered Users
	pushMerge = deny group Registered Users
[access "refs/for/refs/*"]
	push = deny group Registered Users
	push = group rhosqeauto-core
	rebase = group rhosqeauto-core
	pushMerge = deny group Registered Users
	pushMerge = group rhosqeauto-core
[access "refs/heads/*"]
	read = group user/rhosqeauto
	create = group user/rhosqeauto
	push = +force group user/rhosqeauto
	push = group user/yfried-redhat
	pushMerge = group user/rhosqeauto
	forgeCommitter = group user/rhosqeauto
	label-Code-Review = -2..+2 group rhosqeauto-core
	label-Verified = -1..+1 group rhosqeauto-core
	label-Verified = -1..+1 group user/rhosqeauto
	rebase = group rhosqeauto-core
	removeReviewer = group rhosqeauto-core
	submit = group rhosqeauto-core
[access "refs/tags/*"]
	push = +force group user/rhosqeauto
	pushTag = group user/rhosqeauto
	pushSignedTag = group user/rhosqeauto
[project]
[receive]
	requireChangeId = true
	createNewChangeForAllNotInTarget = true
[access "refs/for/refs/stable"]
	push = deny group Registered Users
	push = deny group rhosqeauto-core
	push = group user/rhosqeauto
	push = group user/yfried-redhat
	pushMerge = deny group Registered Users
	pushMerge = deny group rhosqeauto-core
	pushMerge = group user/rhosqeauto
	pushMerge = group user/yfried-redhat
