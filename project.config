[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/rhosqeauto
	owner = group user/yfried-redhat
[access "refs/for/refs/*"]
	read = group user/rhosqeauto
	push = group user/rhosqeauto
	label-Code-Review = -2..+2 group user/rhosqeauto
	label-Verified = -1..+1 group user/rhosqeauto
	rebase = group user/rhosqeauto
	removeReviewer = group user/rhosqeauto
	submit = group user/rhosqeauto
[access "refs/heads/*"]
	read = group user/rhosqeauto
	create = group user/rhosqeauto
	push = +force group user/rhosqeauto
	pushMerge = group user/rhosqeauto
[access "refs/tags/*"]
	push = +force group user/rhosqeauto
	pushTag = group user/rhosqeauto
	pushSignedTag = group user/rhosqeauto
